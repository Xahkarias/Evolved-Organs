<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <success>Always</success>
    <mods>
      <li>Yabils: Space Gremlins</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>

        <li Class="PatchOperationAdd">
          <xpath>/Defs/RecipeDef[defName="Make_Humanoid_Protein"]/fixedIngredientFilter/thingDefs</xpath>
          <value>
            <li>Meat_Alien_Yabil</li>
          </value>
        </li>

        <!-- No unique body def used, just inherits body from humanlike thus all normal EVOparts default just fine-->
      </operations>
    </match>
  </Operation>
</Patch>

